pipeline {
    agent any
    environment    {
     
     PATH = "/var/lib/jenkins/tools/hudson.tasks.Maven_MavenInstallation/maven_3.6.3/bin:$PATH"
    }

    stages {
        stage("git"){
            steps{
                git 'https://github.com/srikanthdata/maven-web-application.git'                
            }
        }
        stage("Build") {
            steps {
                sh "mvn clean package"
            }
        }
     }
}
