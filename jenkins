pipeline {
    agent any
    tools{
        maven 'maven 3.6.3'
        }
   
        stage("git"){
            steps{
                git 'https://github.com/srikanthdata/maven-web-application.git'                
            }
        }
        stage("Build") {
            steps {
                sh "mvn clean package"
            }
        }
     
}
